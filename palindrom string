#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    cin >> s;
    bool P = true;
    for (int i = 0; i < s.length() / 2; ++i) {
        if (s[i] != s[s.length() - 1 - i]) {
            P = false;
        }
    }
    if (P) {
        cout << "YES";
    } else {
        cout << "NO";
    }
    return 0;
}
